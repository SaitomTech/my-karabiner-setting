{
    "title": "Using Japanese_Eisuu Shortcut",
    "rules": [
        {
            "description": "Japanese_Eisuu to four modifiers keys",
            "manipulators": [
                {
                    "from": "japanse_eisuu",
                    "to": [{"key_code": "left_control", "modifiers": ["left_option", "left_command", "left_shift"]}]
                }
            ]
        },
        {
            "description": "Japanese_Eisuu + hjkl to arrow keys",
            "manipulators": [
                {
                    "type": "basic",
                    "to": [{"key_code": "left_arrow"}],
                    "from": {
                        "key_code": "h",
                        "modifiers": {"mandatory": ["left_control", "left_option", "left_command", "left_shift"]}
                    }
                }
            ]
        }
    ]
}
